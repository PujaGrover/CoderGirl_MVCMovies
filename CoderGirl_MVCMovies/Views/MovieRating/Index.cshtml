
@{
    ViewData["Title"] = "Movie Rating";
}

<h1>Movie Rating</h1>
<a asp-controller="MovieRating" asp-action="Create">Add Movie Rating</a>
<table>
    <tr>
        <th>Movie Name</th>
        <th>Movie Rating</th>
    </tr>
    @foreach (var movie in ViewBag.MovieRatings)
    {
    <tr id="movieIdToUse">
        <td>@movie.MovieName</td>
        <td>@movie.Rating</td>
    </tr>

    }
</table>
